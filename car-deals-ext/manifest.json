{
    "name": "Car Deals Extension",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Extension to scrape car deals.",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "tabs",
        "storage",
        "unlimitedStorage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}